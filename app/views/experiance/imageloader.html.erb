
<h1 class="p_heading">Upload Image</h1>



<div class="InnerWrapper">
<%= form_for :user,:url=>search_profile_path, :html => {:multipart => true} do |f| %>
  
  <p>
    <%= f.label :name %><br />
  </p>
  <p>
    <%= f.file_field :avatar %>
  </p>
  
  <p><%= f.submit "Upload" , :class=>"connectButton"%></p>
<% end %>

</div>